	@if [ -d unshared ]; then \
	exit 0; \
	else \
	echo "mkdir unshared"; mkdir unshared; \
	for i in $(SUBDIRS); do \
	j=`echo "$$i" | sed -e 's;[^/]\{1,\};..;g'`; \
	$(MKDIRHIER) "unshared/`dirname $$i`"; \
	echo $(LN) "$$j/$$i/unshared" "unshared/$$i"; \
	$(LN) "$$j/$$i/unshared" "unshared/$$i"; \
	done; \
	fi
	$(_NULLCMD_)

