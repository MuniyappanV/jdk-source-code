libXm.so.$(SOXMREV):  $(OBJS) $(EXTRALIBRARYDEPS)
	$(RM) $@~
	@SONAME=`echo $@ | sed 's/\.[^\.]*$$//'`; set -x; \
	(cd shared; $(CC) -o ../$@~ $(SHLIBLDFLAGS) -Wl,-soname,$$SONAME $(OBJS) $(REQUIREDLIBS) -lc); \
	$(RM) $$SONAME; $(LN) $@ $$SONAME; \
	($(RM) $(BUILDLIBDIR)/$$SONAME; cd $(BUILDLIBDIR); $(LN) $(BUILDINCTOP)/$(CURRENT_DIR)/$$SONAME .)
	$(RM) $@
	$(MV) $@~ $@
	$(RM) libXm.so
	$(LN) $@ libXm.so
	@if [ -d $(BUILDLIBDIR) ]; then set +x; \
	else (set -x; $(MKDIRHIER) $(BUILDLIBDIR)); fi
	$(RM) $(BUILDLIBDIR)/$@
	cd $(BUILDLIBDIR) && $(LN) $(BUILDLIBTOP)/$(CURRENT_DIR)/$@ .
	@if [ -d $(BUILDLIBDIR) ]; then set +x; \
	else (set -x; $(MKDIRHIER) $(BUILDLIBDIR)); fi
	$(RM) $(BUILDLIBDIR)/libXm.so
	cd $(BUILDLIBDIR) && $(LN) $(BUILDLIBTOP)/$(CURRENT_DIR)/libXm.so .

clean::
	@MAJREV=`expr $(SOXMREV) : '\(.*\)\.'`; \
	set -x; $(RM) libXm.so.$$MAJREV
	$(RM) libXm.so.$(SOXMREV) libXm.so


install:: libXm.so.$(SOXMREV)
	@if [ -d $(DESTDIR)$(USRLIBDIR) ]; then set +x; \
	else (set -x; $(MKDIRHIER) $(DESTDIR)$(USRLIBDIR)); fi
	$(INSTALL) $(INSTALLFLAGS) $(INSTBINFLAGS) libXm.so.$(SOXMREV) $(DESTDIR)$(USRLIBDIR)
	@T=`echo $(DESTDIR)$(USRLIBDIR)/libXm.so.$(SOXMREV) | sed 's/\.[^\.]*$$//'`;	set -x; $(RM) $$T && $(LN) libXm.so.$(SOXMREV) $$T
	$(RM) $(DESTDIR)$(USRLIBDIR)/libXm.so
	$(LN) libXm.so.$(SOXMREV) $(DESTDIR)$(USRLIBDIR)/libXm.so

